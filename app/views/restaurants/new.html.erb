<%= form_for @restaurant do |f| %>

<%= f.label :name, "name" %>
<%= f.text_field :name %>

<%= f.label :address, "address" %>
<%= f.text_field :address %>

<%= f.label :phone_number, "phone_number" %>
<%= f.text_field :phone_number %>

<%= f.label :category, "category" %>
<%= f.text_field :category %>

<%= f.submit %>
<% end %>
